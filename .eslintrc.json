{
	"env": {
		"es2021": true,
		"node": true,
		"jest": true
	},
	"extends": ["airbnb-base"],
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"rules": {
		"no-underscore-dangle": "off",
		"no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
		"class-methods-use-this": "off"
	}
}
